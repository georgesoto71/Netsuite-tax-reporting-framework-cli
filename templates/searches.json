{
  "list": [
    {
      "title": "Searches",
      "id": "customsearch_ctr_temp",
      "type": "Transaction",
      "columns": [
        {
          "name": "companyname",
          "join": "customer",
          "summary": "GROUP",
          "feature": "multicurrency"
        },
        {
          "name": "custitem_ste_item_taxitem_type",
          "join": "item",
          "summary": "GROUP",
          "sort": "ASC",
          "configuration": {
            "type": "accountingpreferences",
            "id": "ACCOUNTNUMBERS"
          }
        },
        {
          "name": "formulatext",
          "summary": "GROUP",
          "sort": "ASC",
          "formula": "case when {customer.isperson} = 'T' then {customer.firstname} || {customer.middlename} || {customer.lastname} else {customer.companyname} end",
          "validator": {
            "name": "LocalValidator",
            "script": "SuiteApps/com.netsuite.countrytaxreportstemplate/src/validator/LocalValidator"
          }
        }
      ],
      "filterExpression": [
        ["posting", "is", "T"],
        "AND",
        ["taxline", "is", "F"],
        "AND",
        ["mainline", "is", "F"]
      ],
      "settings": [
        {
          "name": "consolidationtype",
          "value": "NONE",
          "feature": "multicurrency"
        }
      ]
    }
  ]
}
